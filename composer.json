{
    "name": "starcode-krasnodar/yii2-amqp",
    "description": "AMQP protocol extension for Yii2",
    "type": "yii2-extension",
    "require": {
        "yiisoft/yii2": "*",
        "php-amqplib/php-amqplib": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Memory Clutter",
            "email": "memclutter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "starcode\\amqp\\": "src/",
            "starcode\\amqp\\tests\\": "tests/"
        }
    }
}
